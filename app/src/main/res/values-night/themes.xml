<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MyMemories" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/md_orange_900</item>
        <item name="colorPrimaryVariant">@color/md_orange_800</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/md_orange_500</item>
        <item name="colorSecondaryVariant">@color/md_orange_400</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <!--<item name= "android:textViewStyle" > @style/OswaldTextViewStyle </item>
        <item name= "android:buttonStyle" > @style/OswaldButtonStyle </item>-->
        <item name= "android:fontFamily" > @style/OswaldViewStyle </item>
        <item name= "android:textViewStyle" > @style/OswaldTextViewStyle </item>
    </style>
</resources>